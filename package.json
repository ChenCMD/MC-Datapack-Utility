{
	"name": "mc-commander-util",
	"displayName": "MCCommanderUtil",
	"description": "",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mccutil.createDatapackTemplate",
		"onCommand:mccutil.createFile",
		"onCommand:mccutil.scoreOperation"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mccutil.createDatapackTemplate",
				"title": "Create DatapackTemplate"
			},
			{
				"command": "mccutil.createFile",
				"title": "Create Datapack File"
			},
			{
				"command": "mccutil.scoreOperation",
				"title": "Formula to Score Operation"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "mccutil.createFile",
					"when": "explorerResourceIsFolder == true",
					"group": "navigation@4"
				}
			],
			"commandPalette": [
				{
					"command": "mccutil.createFile",
					"when": "false"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
